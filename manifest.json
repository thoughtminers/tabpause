{
  "manifest_version": 3,
  "name": "Tab Pause",
  "description": "Pause Chrome tabs by discarding them. Built by Thoughtminers.",
  "version": "1.0.0",
  "action": {
    "default_title": "Pause / Unpause tab",
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png"
    }
  },
  "background": {
    "service_worker": "sw.js"
  },
  "permissions": [],
  "homepage_url": "https://www.buymeacoffee.com/thoughtminers",
  "author": "thoughtminers",
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
